<pre class="prettyprint lang-js">$(document).ready(function(){
 var $backToTopTxt = "返回顶部",$backToTopEle = $('').appendTo($("body")).text($backToTopTxt).attr("title", $backToTopTxt).click(function() {
  $("html, body").animate({ scrollTop: 0 }, 120);
    }), $backToTopFun = function() {
        var st = $(document).scrollTop(), winh = $(window).height();
        (st &gt; 0)? $backToTopEle.show(): $backToTopEle.hide();    
        if (!window.XMLHttpRequest) {
            $backToTopEle.css("top", st + winh - 166);    
        }
    };
    $(window).bind("scroll", $backToTopFun);
    $backToTopFun();
});
</pre>
<p>
 <span>注：页面中需要先加载jquery，返回顶部代码才能实现</span> 
